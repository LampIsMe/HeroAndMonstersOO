        }else if (a.equals("help")){
            System.out.println("Type 'open' to access your inventory (only works if you have any items)\nType 'stats' to view your health, gold, damage, armor, and speed\nType map to view past entities that you have encountered (only works if you have a map)");
            try{Thread.sleep(2500);}catch(InterruptedException e){System.out.println("got interrupted!");
            }
        }else if (a.equals("cheat")){
            String c = new String();
            for (int i = 0; i < 15; i ++){
                for (int j = 0; j < 15; j++){
                    c = c + " " + map[i][j];
                }
                c = c + "\n";
            }
            System.out.print("\f");
            System.out.println(c);
            try{Thread.sleep(1000);}catch(InterruptedException e){System.out.println("got interrupted!");
            }

        }
        if(probability(promap[ethan.getx()][ethan.gety()]) == true){
            if (counter <=18){
                Monster mon = new Monster(6);
                orientation = 0;
                int e = mon.battle(ethan.gethealth(), ethan.herodamage(),  ethan.getDefence(), ethan.getspeed(), orientation, inv);
                if(e==0){
                    System.out.println("GAME OVER");
                    return "failed";
                }
                else if (e==1010){
                    System.out.println("Your remaining health is " + mon.gethhealth());
                    ethan.updateHealth(mon.gethhealth());
                    printedMap[mon.getx()][mon.gety()]="XXXX";
                }
                else {
                    System.out.println("Your remaining health is " + mon.gethhealth());
                    ethan.updateHealth(mon.gethhealth());
                    map[mon.getx()][mon.gety()]=0;
                    printedMap[mon.getx()][mon.gety()]="XXXX";
                    counter++;
                    ethan.addGold(10);
                }
            }
        }

        
        
        updateMap();
        updateprintedMap(0,0,"XXXX");
        printMap2(printedMap, b);
        if (bosscounter==4){
            return "success";
        }
        else if (ethan.gethealth()==0){
            return "failed";
        }else{
            return "playing";
        }
    }

    public void printMap2(String[][]map, String b){
        for (int i = 0; i < 15; i ++){
            for (int j = 0; j < 15; j++){
                b = b + " " + printedMap[i][j];
            }
            b = b + "\n";
        }
        System.out.print("\f");
        System.out.println(b);
        b = null;
    }

    public void updateMap(){
        for (int i = 0; i < monsterArray.size(); i ++){
            if(map[monsterArray.get(i).getx()][monsterArray.get(i).gety()]!=1){
                monsterArray.remove(monsterArray.get(i));

            }else {continue;};
        }
        for (int i = 0; i < potionArray.size(); i ++){
            if(map[potionArray.get(i).getx()][potionArray.get(i).gety()]!=2){
                potionArray2.add(potionArray.get(i));
                potionArray.remove(potionArray.get(i));

            };
        }
        for (int i = 0; i < villagerArray.size(); i ++){
            if(map[villagerArray.get(i).getx()][villagerArray.get(i).gety()]!=5){
                //villagerArray.remove(villagerArray.get(i));
            };
        }
        
       
    }

    public void updateprintedMap(int x, int y, String a){
        printedMap[x][y] = a;
        printedMap[ethan.getx()][ethan.gety()] = "HERO";

    }

    public int returnHealth(){
        return ethan.gethealth();
    }

    public boolean probability(double pro){
        double random = (int)(Math.random() * (101)) + 0;
        random = random * pro;

        //try{Thread.sleep(1000);}catch(InterruptedException e){System.out.println("got interrupted!");
        //   }
        if (random >= 50){
            return true;
        }else {
            return false;  
        }
    }
}
