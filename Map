                    int e = monsterArray.get(rand).battle(ethan.gethealth(), ethan.herodamage(), ethan.getDefence(), ethan.getspeed(), orientation, inv);

                    if(e==0){
                        System.out.println("GAME OVER");
                        return "failed";
                    }
                    else if (e==1010){
                        System.out.println("Your remaining health is " + monsterArray.get(rand).gethhealth());
