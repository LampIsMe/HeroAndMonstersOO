        int rand=0;
        //-1,0 = up 
        int orientation;
        if (counter>= 1 && bosscounter == 0){

            Kid kid1 = new Kid();
            bossArray.add(kid1);
            
            try{Thread.sleep(200);}catch(InterruptedException e){System.out.println("got interrupted!");
            }
        }
        else if (counter>= 2 && bosscounter == 1){
            
            Teen teen1 = new Teen();
            bossArray.add(teen1);
            
            try{Thread.sleep(200);}catch(InterruptedException e){System.out.println("got interrupted!");
            }
        }
        else if (counter >= 3&& bosscounter == 2){

            Parent parent1 = new Parent();
